{
  "expo": {
    "name": "FleetPulse",
    "slug": "fleetpulse",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo/fleetpulse-icon.png",
    "splash": {
      "image": "./assets/logo/fleetpulse-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourcompany.fleetpulse"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo/fleetpulse-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.yourcompany.fleetpulse"
    },
    "web": {
      "favicon": "./assets/logo/fleetpulse-icon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#000000",
          "image": "./assets/logo/fleetpulse-splash.png",
          "resizeMode": "contain",
          "dark": {
            "backgroundColor": "#000000",
            "image": "./assets/logo/fleetpulse-splash.png",
            "resizeMode": "contain"
          },
          "light": {
            "backgroundColor": "#FFFFFF",
            "image": "./assets/logo/fleetpulse-splash-light.png",
            "resizeMode": "contain"
          }
        }
      ],
      "expo-mail-composer"
    ]
  }
}
